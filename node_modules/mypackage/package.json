{
  "name": "mypackage",
  "version": "1.0.0",
  "description": "A Package for module testing",
  "maintainers": [
    {
      "name": "Maintainers Team",
      "email": "maintainers@example.com"
    }
  ],
  "bugs": {
    "mail": "bugs@example.com"
  },
  "dependencies": {
    "moat": "~2.0.0"
  },
  "main": "./index.js",
  "private": true,
  "notification": { "http" : "http://localhost:3000/request_histories" },
  "models" : {
    "FirstLargeObject" : {
      "array" : true,
      "scope" : "user",
      "attributes" : {
        "name" : {"type" : "string"},
        "timestamp" : {"type" : "int64"},
        "file" : {"type" : "resource"},
        "binary" : {"type" : "binary"}
      },
      "commands" : {
        "downloadToLocal" : { "paramType" : "string" },
        "removeLocal" : { "paramType" : "string" },
        "uploadFromLocal" : { "paramType" : "string" },
        "collectBinary" : { "paramType" : "string" }
      }
    },
    "SecondLargeObject" : {
      "array" : true,
      "scope" : "user",
      "attributes" : {
        "name" : {"type" : "string"},
        "prop1" : {"type" : "int64"},
        "prop2" : {"type" : "resource"},
        "prop3" : {"type" : "binary"}
      },
      "commands" : {
        "cmd1" : { "paramType" : "string" },
        "cmd2" : { "paramType" : "string" },
        "cmd3" : { "paramType" : "string" },
        "cmd4" : { "paramType" : "string" }
      }
    }
  }
}
